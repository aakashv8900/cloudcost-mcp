{
    "version": 2,
    "builds": [
        {
            "src": "api/index.ts",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/health",
            "dest": "api/index.ts"
        },
        {
            "src": "/mcp",
            "dest": "api/index.ts"
        },
        {
            "src": "/sse",
            "dest": "api/index.ts"
        },
        {
            "src": "/messages",
            "dest": "api/index.ts"
        },
        {
            "src": "/",
            "dest": "api/index.ts"
        }
    ],
    "functions": {
        "api/index.ts": {
            "memory": 1024,
            "maxDuration": 30
        }
    }
}